# $Header$

start() {
	ebegin "Server/Node high-level initialisation"
	if checkserver
	then
		ewarn "  Server detected." && /sbin/rc server_default
	else
		ewarn "  Node detected." && /sbin/rc node_default
	fi
	eend 0
}

